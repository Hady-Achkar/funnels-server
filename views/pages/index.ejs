<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head.ejs'); %>
  <body>
    <main class="bg-slate-50">
      <div class="max-w-3xl mx-auto py-8 shadow sm:rounded-lg p-2 bg-white">
        <ul style="list-style: none;padding-top: 20px;" class=" pb-8 flex items-center justify-between px-6" >
          <a href="/" class="font-semibold text-xl capitalize text-gray-700 hover:text-gray-600"><%= funnelTitle %></a>
          <% if (headerMenue.links) { %>
            <div>
            <% for(var i=0; i<headerMenue.links.length; i++) {%>
              <li style=" display: inline; margin: 5px;">
                <a class="nav-link capitalize text-md text-gray-900 hover:underline" href="<%= headerMenue.links[i].href %>"><%= headerMenue.links[i].title %></a>
              </li>
            <% } %>
            </div>
          <% } else{ %>
            <div></div>
          <% } %>
        </ul>
        <div class="px-4">
      <%- html %>
        </div>
      <footer  aria-labelledby="footer-heading">
        <div class="mt-8 border-t border-gray-200 pt-8">
          <p class="text-base text-gray-400 xl:text-center">Powered by <a href="https://www.funnelshero.com" class="text-indigo-700 hover:text-indigo-600">Funnelshero</a></p>
        </div>
    </footer>
      </div>
    </main>
  </body>
</html>


