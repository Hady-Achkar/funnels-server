<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head.ejs'); %>
  <body>
    <main>
      <div class="max-w-3xl mx-auto bg-slate-50">
        <ul style="list-style: none;padding-top: 20px;" >
          <% if (headerMenue) { %>
            <div>
            <% for(var i=0; i<headerMenue.links.length; i++) {%>
              <li style=" display: inline; margin: 5px;">
                <a class="nav-link" href="<%= headerMenue.links[i].href %>"><%= headerMenue.links[i].title %></a>
              </li>
            <% } %>
            </div>
          <% } %>
        </ul>
      <%- html %>
        <ul style="list-style: none;padding-top: 20px;" >
          <% if (footerMenue) { %>
          <div>
            <% for(var i=0; i<footerMenue.links.length; i++) {%>
              <li style=" display: inline; margin: 5px;">
                <a class="nav-link" href="<%= headerMenue.links[i].href %>"><%= headerMenue.links[i].title %></a>
              </li>
            <% } %>
          </div>
          <% } %>
        </ul>
      </div>
    </main>
  </body>
</html>
